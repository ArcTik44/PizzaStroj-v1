<h1>My Orders</h1>
<table class="table table-stripe">
<thead>
<tr>Email</tr>
<tr>Order Time</tr>
<tr>Price</tr>
<tr>Status</tr>
</thead>
<tbody>
{{#orders}}
<tr>
    <td>{{email}}</td>
    <td>{{order_time}}</td>
    <td>{{price}}$</td>
    <td>{{state}}</td>
    <td><a href="orderDetail.php?order_id={{order_id}}">Detail</a></td>
</tr>
{{/orders}}
</tbody>
</table>
<a href = "index.php">Back</a>